{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=multitenantdb;Username=postgres;Password=postgres",
    "MongoDb": "mongodb://admin:admin123@localhost:27017"
  },
  "JWT": {
    "ValidAudience": "http://localhost:5000",
    "ValidIssuer": "http://localhost:5000",
    "Secret": "ThisIsASecretKeyForJwtTokenGeneration123!"
  },
  "Cache": {
    "Enabled": true,
    "Redis": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "MultiTenantApp:",
      "ConnectTimeout": 5000,
      "SyncTimeout": 5000,
      "AbortOnConnectFail": false,
      "ConnectRetry": 3
    },
    "Defaults": {
      "DefaultExpirationMinutes": 60,
      "SlidingExpirationMinutes": 30,
      "UseDistributedCache": true
    }
  },
  "RateLimit": {
    "Enabled": true,
    "Global": {
      "Enabled": true,
      "Limit": 1000,
      "WindowMinutes": 1
    },
    "PerTenant": {
      "Enabled": true,
      "Limit": 500,
      "WindowMinutes": 1
    },
    "PerUser": {
      "Enabled": true,
      "Limit": 100,
      "WindowMinutes": 1
    },
    "PerIp": {
      "Enabled": true,
      "Limit": 100,
      "WindowMinutes": 1
    },
    "Endpoints": [
      {
        "Endpoint": "/api/Auth/login",
        "Limit": 10,
        "WindowMinutes": 1,
        "HttpMethods": [
          "POST"
        ]
      }
    ]
  },
  "EmailSettings": {
    "SmtpServer": "localhost",
    "SmtpPort": 1025,
    "SenderEmail": "noreply@multitenantapp.com",
    "SenderName": "MultiTenant App",
    "Username": "",
    "Password": "",
    "EnableSsl": false
  },
  "FileStorage": {
    "FileBrowserUrl": "http://localhost:8081",
    "FileBrowserUsername": "admin",
    "FileBrowserPassword": "admin"
  },
  "AppUrl": "http://localhost:5173",
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4317"
}