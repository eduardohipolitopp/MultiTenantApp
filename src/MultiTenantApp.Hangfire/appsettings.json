{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=multitenantapp;Username=postgres;Password=password"
  },
  "Hangfire": {
    "Dashboard": {
      "Title": "MultiTenantApp - Background Jobs"
    },
    "Server": {
      "WorkerCount": 5
    }
  },
  "Observability": {
    "ServiceName": "MultiTenantApp.Hangfire",
    "ServiceVersion": "1.0.0",
    "OtlpEndpoint": "http://otel-collector:4317",
    "EnableConsoleExporter": true
  }
}